class JointPose
{
    private:
    vector<float> joint_positions;
    std::list<float> joint_positions(6); 

    public:
    std::string poseName;
 
    void printinfo()
    {
       std::cout << "pose name: " << std::poseName << std::endl;
         for (double i: joint_positions):
            std::cout << "joint[" i ] << i << std::endl
            std::cout << std::format("Joint[{}] = {}", i, joint_positions[i]);
    }
};

